{
    "compilerOptions": {
        "module": "commonjs",
        "allowJs": true,
        "outDir": "dist",
        "target": "es5",
        "noImplicitAny": true,
        "noEmit": false,
        "strictNullChecks": true,
        "suppressExcessPropertyErrors": false,
        "suppressImplicitAnyIndexErrors": false,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "lib": [
            "es2015",
            "dom"
        ]
    },
    "files": [
        "./node_modules/**/*.d.ts"
    ]
}
